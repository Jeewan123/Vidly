@using Vidly.Models
@model IEnumerable<Vidly.Models.Customer>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customer</h2>
<div>
    <table class="table table-bordered">
        <thead>
        <th>Customers</th>
        <th>MembershipType</th>
        </thead>
        <tbody>
        @foreach (var custom in Model)
        {
            <tr>
                <td>@Html.ActionLink(custom.Name,"Edit","Customers",new {id=custom.Id},null) </td>
                <td>@custom.MembershipType.MembershipName</td>
            </tr>
        }
        </tbody>

    </table>
</div>

